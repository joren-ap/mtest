<%- include("header") -%>

<h1 class="coll">Our collection</h1>

<div class="button-group filters-button-group">
  <button class="button is-checked" data-filter="*">All</button>
  <button class="button" data-filter=".1960">1960s</button>
  <button class="button" data-filter=".1970">1970s</button>
  <button class="button" data-filter=".1980">1980s</button>
  <button class="button" data-filter=".1990">1990s</button>
</div>

<div class="grid">
<% for (let i=0; i<posts.length; i++) { %>
    <section>
      <%- include('kunstwerk', {post: posts[i], index: i}); -%>
    </section>
  <% } // close forloop %>
</div>

<%- include("footer") -%>  
